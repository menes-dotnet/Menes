<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="$(EndjinProjectPropsPath)" Condition="$(EndjinProjectPropsPath) != ''" />

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <NoWarn>RCS1029;CS1591</NoWarn>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn>RCS1029;CS1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <SpecFlowObsoleteCodeBehindFiles Remove="Features\JsonModel\Cast\JsonArrayCast - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Features\JsonModel\Cast\JsonBase64ContentCast - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Features\JsonModel\Cast\JsonBase64StringCast - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Features\JsonModel\JsonArrayEquals - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Features\JsonModel\JsonDateEquals - Copy.feature.cs" />
    <SpecFlowObsoleteCodeBehindFiles Remove="Features\JsonModel\JsonTimeEquals - Copy.feature.cs" />
  </ItemGroup>



  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="5.0.0" />
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.9.4" />
    <PackageReference Include="SolidToken.SpecFlow.DependencyInjection" Version="3.7.0" />
    <PackageReference Include="SpecFlow.Plus.LivingDocPlugin" Version="3.7.141" />
    <PackageReference Include="System.Text.Encodings.Web" Version="5.0.1" />

    <PackageReference Include="Endjin.RecommendedPractices" Version="1.2.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.9.0" />


    <PackageReference Include="SpecFlow.NUnit" Version="3.7.38" />
    <PackageReference Include="nunit" Version="3.13.1" />
    <PackageReference Include="NUnit3TestAdapter" Version="3.17.0" />





    <PackageReference Include="FluentAssertions" Version="5.10.3" />

  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Menes.Json.SchemaModel\Menes.Json.SchemaModel.csproj" />
    <ProjectReference Include="..\Menes.Json\Menes.Json.csproj" />
    <ProjectReference Include="..\Menes.OpenApi.SchemaModel\Menes.OpenApi.SchemaModel.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Features\JsonModel\Cast\JsonContentCast.feature.cs">
      <DependentUpon>JsonContentCast.feature</DependentUpon>
    </Compile>
    <Compile Update="Features\JsonModel\Cast\JsonBase64StringCast.feature.cs">
      <DependentUpon>JsonBase64StringCast.feature</DependentUpon>
    </Compile>
    <Compile Update="Features\JsonModel\Cast\JsonBase64ContentCast.feature.cs">
      <DependentUpon>JsonBase64ContentCast.feature</DependentUpon>
    </Compile>
    <Compile Update="Features\JsonModel\Cast\JsonArrayCast.feature.cs">
      <DependentUpon>JsonArrayCast.feature</DependentUpon>
    </Compile>
    <Compile Update="Features\JsonModel\Equals\JsonUuidEquals.feature.cs">
      <DependentUpon>JsonUuidEquals.feature</DependentUpon>
    </Compile>
    <Compile Update="Features\JsonModel\Equals\JsonTimeEquals.feature.cs">
      <DependentUpon>JsonTimeEquals.feature</DependentUpon>
    </Compile>
  </ItemGroup>


  <ItemGroup>
    <None Update="appsettings.local.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="appsettings.template.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="appsettings.local.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>


  <ItemGroup>
    <SpecFlowFeatureFiles Update="Features\JsonModel\Cast\JsonContentCast.feature">
      <Visible>$(UsingMicrosoftNETSdk)</Visible>
      <CodeBehindFile>%(RelativeDir)%(Filename).feature$(DefaultLanguageSourceExtension)</CodeBehindFile>
    </SpecFlowFeatureFiles>
    <SpecFlowFeatureFiles Update="Features\JsonModel\Cast\JsonBase64StringCast.feature">
      <Visible>$(UsingMicrosoftNETSdk)</Visible>
      <CodeBehindFile>%(RelativeDir)%(Filename).feature$(DefaultLanguageSourceExtension)</CodeBehindFile>
    </SpecFlowFeatureFiles>
    <SpecFlowFeatureFiles Update="Features\JsonModel\Cast\JsonBase64ContentCast.feature">
      <Visible>$(UsingMicrosoftNETSdk)</Visible>
      <CodeBehindFile>%(RelativeDir)%(Filename).feature$(DefaultLanguageSourceExtension)</CodeBehindFile>
